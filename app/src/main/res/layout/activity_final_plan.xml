<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FinalPlan">


    <!-- Horizontal Margins -->

    <!-- Vertical Margins -->

    <!-- Content -->

    <ImageView
        android:src="@drawable/finalplan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        />



    <View
        android:id="@+id/horizontalMargin1"
        android:layout_width="90dp"
        android:layout_height="match_parent"
        />

    <View
        android:id="@+id/verticalMargin1"
        android:layout_width="match_parent"
        android:layout_height="14dp"
        />

    <Button
        android:id="@+id/returnHome"
        android:layout_width="14dp"
        android:layout_height="15dp"
        android:layout_toRightOf="@id/horizontalMargin1"
        android:layout_below="@id/verticalMargin1"
        android:background="@android:color/transparent"
        />




    <View
        android:id="@+id/horizontalMargin2"
        android:layout_width="220dp"
        android:layout_height="match_parent"
        />

    <View
        android:id="@+id/verticalMargin2"
        android:layout_width="match_parent"
        android:layout_height="465dp"
        />

    <Button
        android:id="@+id/share"
        android:layout_width="70dp"
        android:layout_height="25dp"
        android:layout_toRightOf="@id/horizontalMargin2"
        android:layout_below="@id/verticalMargin2"
        android:background="@android:color/transparent"
        />

</RelativeLayout>